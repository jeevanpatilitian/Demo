pipeline {
    agent {
        docker  {
            image 'javasbapp'
            args '-v $HOME/.m2:/root/.m2'
        }
    }
    stages {
        stage('Build') {
            steps {
                echo "Hello Word"
                sh 'java -version'
                sh 'mvn -f demo/pom.xml clean install'
                  
            }
        }
    }
}
